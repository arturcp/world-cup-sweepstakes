<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"> 

<%= content_tag :section, nil, id: 'ranking-container' do %>
  <div class="row">
    <div class="col s12 l4 offset-l4 center-align">
      <img src="<%= @tournament.logo %>" class="ranking-tournament-logo" />
    </div>
  </div>
  <div class="row">
    <div class="col s12 l4 offset-l4 center-align">
      <div class="card">
        <div class="card-content">
          <%= render 'ranking_table' %>
        </div>
      </div>
    </div>
  </div>
<% end %>
